# LiveKit API Configuration
LIVEKIT_API_KEY=YOUR_API_KEY
LIVEKIT_API_SECRET=YOUR_API_SECRET

# Public configuration
NEXT_PUBLIC_LIVEKIT_URL=wss://YOUR_LIVEKIT_URL

# Application Configuration
NEXT_PUBLIC_APP_CONFIG="
title: 'Voicer v1.0.2'
video_fit: 'cover' # 'contain' or 'cover'
settings:
  editable: false # Should the user be able to edit settings in-app
  theme_color: 'cyan'
  chat: false # Enable or disable chat feature
  outputs:
    audio: true # Enable or disable audio output
    video: false # Enable or disable video output
  inputs:
    mic: true # Enable or disable microphone input
    camera: false # Enable or disable camera input
    sip: false # Enable or disable SIP input
"
